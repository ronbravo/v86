{
  "name": "v86",
  "license": "BSD-2-Clause",
  "type": "module",
  "scripts": {
    "build:bin": "cd src/asm && make",
    "build:lib": "make build/libv86.js",
    "build:defasm": "npx rollup --config rollup.config.defasm.js",
    "build:image": "node src/js/server/build/image/start.js",
    "dev": "npx nodemon --exec \"npm run dev:parcel\" --watch build/sandbox -e bin",
    "dev:build": "npx nodemon --exec \"node src/js/server/watch/start.js\" --watch src/js/server/watch",
    "dev:api": "npx nodemon --exec \"node src/js/server/start.js\" --watch src/js/server",
    "dev:os": "npx live-server --port=9502 --cors --no-browser --watch=src ./",
    "dev:parcel": "npx parcel serve --port=9500 public/index.html",
    "test": "node tests/full/run.js"
  },
  "devDependencies": {
    "@defasm/core": "^2.3.2",
    "@parcel/transformer-less": "2.11.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "ass-js": "^2.1.1",
    "buffer": "^5.5.0||^6.0.0",
    "nodemon": "^3.0.3",
    "parcel": "^2.11.0",
    "rollup": "^4.10.0"
  },
  "dependencies": {
    "axios": "^1.6.3",
    "cors": "^2.8.5",
    "fs-extra": "^11.2.0",
    "lemonadejs": "^4.1.1",
    "less": "^4.2.0",
    "litedom": "^0.12.1",
    "minimist": "^1.2.8",
    "restana": "^4.9.7",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4"
  }
}
